{
  "name": "rickandmortyapi-sql",
  "version": "1.0.0",
  "description": "A clone of the Rick and Morty API with PostgreSQL and Express",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "start:dev": "nodemon ./app.js",
    "migrations:up": "node ./src/database/migrations/up.js",
    "migrations:down": "node ./src/database/migrations/down.js",
    "seeds": "node ./src/database/seeders/run.js"
  },
  "keywords": [
    "POO",
    "JavaScript",
    "API",
    "SQL",
    "Docker"
  ],
  "author": "PJuanCruz",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "2.0.22"
  },
  "dependencies": {
    "dotenv": "16.1.4",
    "express": "4.18.2",
    "node-fetch": "3.3.1",
    "pg": "8.11.0"
  }
}
